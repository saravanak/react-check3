{
  "name": "react-check3",
  "version": "0.1.1",
  "description": "React Checkbox with 3 states",
  "main": "dist/react-check3.js",
  "scripts": {
    "test": "make",
    "lib": "gulp",
    "build-style": "webpack --progress --colors --config build-style.config.js",
    "dist": "webpack --progress --colors --config dist.config.js",
    "dist.min": "webpack --progress --colors --optimize-minimize --optimize-occurence-order --optimize-dedupe --config dist.min.config.js",
    "build": "npm run lib && npm run dist && npm run dist.min",
    "serve": "http-server -p 9091",
    "dev": "webpack-dev-server --progress --colors --port 9090 --content-base http://localhost:9091"
  },
  "devDependencies": {
    "css-loader": "^0.9.0",
    "extract-text-webpack-plugin": "^0.3.5",
    "file-loader": "^0.8.1",
    "gulp": "^3.8.11",
    "gulp-react": "^3.0.0",
    "gulp-webpack": "^1.2.0",
    "gulp-webpack-build": "^0.2.6",
    "http-server": "^0.7.3",
    "jsx-loader": "^0.12.2",
    "style-loader": "^0.8.2",
    "stylus-loader": "^0.4.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.6"
  },
  "dependencies": {
    "object-assign": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radubrehar/react-check3.git"
  },
  "keywords": [
    "checkbox",
    "check",
    "input",
    "3",
    "states",
    "react",
    "react-component"
  ],
  "author": "Radu Brehar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radubrehar/react-check3/issues"
  },
  "homepage": "https://github.com/radubrehar/react-check3"
}
